<!-- Funfact Area
============================================ -->
{% set veh = 0 %}
{% set agen = 0 %}
{% set moni = 0 %}
{% set cli = 0 %}

{% for vehicule in vehicule %}
    {% set veh = veh + 1 %}
{% endfor %}
{% for moniteur in moniteur %}
    {% set moni = moni + 1 %}
{% endfor %}
{% for agence in agences %}
    {% set agen = agen + 1 %}
{% endfor %}
{% for client in client %}
    {% set cli = cli + 1 %}
{% endfor %}
<div class="funfact-area overlay overlay-white overlay-80 pt-90 pb-60">
    <div class="container">
        <div class="row">
            <div class="single-facts text-center col-sm-3 col-xs-12 mb-30">
                <i class="icofont icofont-auto-mobile"></i>
                <h1 class="counter">{{ veh }}</h1>
                <p>Nombre de vehicule</p>
            </div>
            <div class="single-facts text-center col-sm-3 col-xs-12 mb-30">
                <i class="icofont icofont-ui-user-group"></i>
                <h1 class="counter">{{ moni }}</h1>
                <p>Nombre de moniteur</p>
            </div>
            <div class="single-facts text-center col-sm-3 col-xs-12 mb-30">
                <i class="icofont icofont-company"></i>
                <h1 class="counter">{{ agen }}</h1>
                <p>Nombre d'agence</p>
            </div>
            <div class="single-facts text-center col-sm-3 col-xs-12 mb-30">
                <i class="icofont icofont-users-social"></i>
                <h1 class="counter">{{ cli }}</h1>
                <p>Nombre de client</p>
            </div>
        </div>
    </div>
</div>

