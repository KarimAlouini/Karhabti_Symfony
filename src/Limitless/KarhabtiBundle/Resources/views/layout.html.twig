                        <div class="header-top-left text-left col-sm-6">
                            {% if (user.telephone is defined) %}
                                <p><i class="icofont icofont-ui-call"></i><span>(+216) {{ user.telephone }}</span></p>
                            {% endif %}
                            {% if (user.ouverture is defined) %}
                                <p><i class="icofont icofont-clock-time"></i><span>{{ user.ouverture }}
