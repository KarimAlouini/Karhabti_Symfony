                <div class="contact-form form text-left col-md-8 col-sm-7 col-xs-12"style="width: 100%">
                    <style type="text/css">
                        button {
                            background-color: #dec839; /* Green */
                            border: none;
                            color: white;
                            padding: 10px 15px;
                            text-align: center;
                            text-decoration: none;
                            display: inline-block;
                            font-size: 16px;
                        }
                    </style>



                    {{ form_start(form) }}

                        <br class="input-2">


                            <div class="input">{{ form_widget(form.nom,{'attr':{'placeholder':'Nom'}}) }}</div>
                            <div class="input">{{ form_widget(form.prenom,{'attr':{'placeholder':'Prenom'}}) }}</div>
                            <div class="input">{{ form_widget(form.adresse,{'attr':{'placeholder':'Adresse'}}) }}</div>
                            <div class="input">{{ form_widget(form.ville,{'attr':{'placeholder':'Ville'}}) }}</div>
                            <div class="input">{{ form_widget(form.telephone,{'attr':{'placeholder':'Telephone'}}) }}</div>
                            <div class="input">{{ form_widget(form.dateNaissance,{'attr':{'placeholder':'DateNaissance'}}) }}</div>
                            <div class="input">{{ form_widget(form.codePostal,{'attr':{'placeholder':'CodePostale'}}) }}</div>
